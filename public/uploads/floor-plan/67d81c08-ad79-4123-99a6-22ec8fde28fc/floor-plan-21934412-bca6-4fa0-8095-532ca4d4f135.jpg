<script>
	// show/hide dropdown menu
	function toggleDropdownMenu(menuBtn) {
		const mb = document.querySelector('body').clientWidth < 992;
		const mbSelector = mb ? '-mobile' : '';

		// set chevron down icon for all menu buttons
		document.querySelector('.dropdown-menu-element').classList.remove('goes-up');
		document.querySelector('.dropdown-menu-element').classList.remove('menu-active');
		document.querySelector('.dropdown-menu-element').classList.add('goes-down');

		if (document.querySelector('#' + menuBtn + '-list-block' + mbSelector).clientHeight > 0) {
		  hideDropDownMenu(menuBtn, mbSelector);
		} else {
			// first close all dropdowns
			const openedDropdown = document.querySelector(".hp-header__dropdown[id*='-list-block']:not(.d-none)")

			if (openedDropdown) {
			  const openedMenuBtn = openedDropdown.id.replace('-list-block-mobile', '').replace('-list-block', '');
			  hideDropDownMenu(openedMenuBtn, mbSelector);
			}

			showDropDownMenu(menuBtn, mbSelector);
		}
	}

	function showDropDownMenu(menuBtn, mbSelector) {
	  // open the selected dropdown
	  document.querySelector('#' + menuBtn + '-list-block' + mbSelector).classList.remove('d-none');

	  // update menu button icon
	  document.querySelector('#' + menuBtn + '-menu-btn' + mbSelector).classList.add('goes-up');
	  document.querySelector('#' + menuBtn + '-menu-btn').classList.add('menu-active');
	  document.querySelector('#' + menuBtn + '-menu-btn' + mbSelector).classList.remove('goes-down');

	  window.openedMenuBtn = menuBtn;
	  window.openedMenuBtnMbSelector = mbSelector;
	}

	function hideDropDownMenu(menuBtn, mbSelector) {
	  // its already open, close dropdown
	  document.querySelector('#' + menuBtn + '-list-block' + mbSelector).classList.add('d-none');

	  // update menu button icon
	  document.querySelector('#' + menuBtn + '-menu-btn' + mbSelector).classList.add('goes-down');
	  document.querySelector('#' + menuBtn + '-menu-btn' + mbSelector).classList.remove('goes-up');
	  document.querySelector('#' + menuBtn + '-menu-btn').classList.remove('menu-active');

	  window.openedMenuBtn = null;
	  window.openedMenuBtnMbSelector = null;
	}

	document.body.addEventListener("click", function() {    
		//Menu is open and click is not the menu button
		if (window.openedMenuBtn && !event.target.closest('li.dropdown-menu-element')) {
			
			const dropdownMenu = document.getElementById(window.openedMenuBtn + '-list-block' + window.openedMenuBtnMbSelector).querySelector('.hp-header__dropmenu-wrapper')        

			if (dropdownMenu) {
				const dropdownMenuRect = dropdownMenu.getBoundingClientRect();
				const clickX = event.clientX;
				const clickY = event.clientY;

				if (clickX >= dropdownMenuRect.left && clickX <= dropdownMenuRect.right && clickY >= dropdownMenuRect.top && clickY <= dropdownMenuRect.bottom) {
					//The click is inside the drop-down menu or its margins, so do nothing
					return
				}

				hideDropDownMenu(window.openedMenuBtn, window.openedMenuBtnMbSelector);
			}
		}
	});

</script>

