		<script>
			var cardSwiperConfig = {
				loop: true,
				autoplay: false,
				navigation: {
					nextEl: ".swiper-button-next",
					prevEl: ".swiper-button-prev",
				},
				pagination: {
					enabled: false 
				},
				on: {
					init: function (swiper_el) {
						let dots_size = 16;

						let dots_width = dots_size * 5;

						if (swiper_el.slides.length < 5) {
							dots_width = dots_size * swiper_el.slides.length;
						}

						swiper_el.el
							.querySelector(".swiper-pagination")
							.insertAdjacentHTML('afterend', '<div class="swiper-dots" style="width:' + dots_width + 'px"><div class="swiper-dots-slider"></div></div>');

						for (let i = 0; i < swiper_el.slides.length; i++) {
							let dot = document.createElement('a');
							dot.classList.add('swiper-dot');
							dot.setAttribute('data-slide', i);
							
							if(swiper_el.realIndex == i) {
								dot.classList.add('active-dot');
							}

							dot.addEventListener('click', function (e) {
								e.preventDefault();
								console.log("slide to:" + i);
								swiper_el.slideToLoop(i);
							});

							swiper_el.el.querySelector(".swiper-dots-slider").appendChild(dot);
						}

						shrinkSwiperDots(swiper_el, 0);
					},
					slideChange : function(swiper_el) {
						let dots = swiper_el.el.querySelectorAll(".swiper-dot");
						for (let i = 0; i < dots.length; i++) {
							dots[i].classList.remove('active-dot');
						}

						dots[swiper_el.realIndex].classList.add('active-dot');

						let first_visible_dot_index = swiper_el.realIndex - 2;
						swiper_el.el.querySelector(".swiper-dots").scrollTo({
							left: first_visible_dot_index * 16,
							behavior: 'smooth'
						});

						shrinkSwiperDots(swiper_el, first_visible_dot_index);
					}
				}
			};      

			function shrinkSwiperDots(swiper_el, first_visible_dot_index) {
				let small_dots = swiper_el.el.querySelectorAll(".small-dot");

				for (let i = 0; i < small_dots.length; i++) {
					small_dots[i].classList.remove('small-dot');
				}

				let last_visible_dot_index = first_visible_dot_index + 4;

				if (first_visible_dot_index > swiper_el.slides.length - 5) {
					first_visible_dot_index = swiper_el.slides.length - 5;
				}

				if (first_visible_dot_index > 0) {
					swiper_el.el.querySelectorAll(".swiper-dot")[first_visible_dot_index].classList.add('small-dot');
				}

				if (last_visible_dot_index < 5) {
					last_visible_dot_index = 4;
				}

				if (last_visible_dot_index < swiper_el.slides.length - 1) {
					swiper_el.el.querySelectorAll(".swiper-dot")[last_visible_dot_index].classList.add('small-dot');
				}
			}

			var cardContainerSwiperConfig = {
				slidesPerView: 1,
				spaceBetween: 30,
				navigation: {
					nextEl: ".outer-swiper-button-next",
					prevEl: ".outer-swiper-button-prev",
				},

				breakpoints: {
					993: {
						slidesPerView: 3,
						spaceBetween: 20,
					},
					769: {
						slidesPerView: 2,
						spaceBetween: 35,
					},
				},
			};
		</script>

