<script>
	var adding_favorite = undefined;

	window.addEventListener("DOMContentLoaded", () => {
		let authModal = document.querySelector("#authModal");
		let showAuthModal = document.querySelectorAll("#showAuthModal");
		let closeBtn = document.querySelector("#closeBtn");
		let socialSignin = document.querySelector("#social-signin");
		let forgetPass = document.querySelector("#forgetPass");
		let passwordSignin = document.querySelector("#passwordSignin");
		let magicLoginWrapper = document.querySelector("#magic-login-wrapper");
		let passwordLoginWrapper = document.querySelector("#password-login-wrapper");
		let forgetPassWrapper = document.querySelector(".forget-pass-wrapper");
		let hpHeader = document.querySelector(".hp-header");

		showAuthModal.forEach((modalBtn) => {
			modalBtn.addEventListener("click", (event) => {
				if (event.target.closest(".favorite-logged-out")) {
					let plan_id = Array.from(event.target.closest(".favorite-logged-out").classList)
						.find((c) => c.includes("plan-"))
						.replace(/\D/g, "");
					adding_favorite = plan_id;
				}
				authModal.classList.add("openAuthModal");
				hpHeader.style.zIndex = "0";
			});
		});

		closeBtn.addEventListener("click", () => {
			authModal.classList.remove("openAuthModal");
			hpHeader.style.zIndex = "500";
		});

		passwordSignin.addEventListener("click", (e) => {
			e.preventDefault();
			passwordLoginWrapper.classList.remove("remove");
			magicLoginWrapper.classList.add("remove");
			socialSignin.classList.add("remove");
		});

		forgetPass.addEventListener("click", (e) => {
			e.preventDefault();
			passwordLoginWrapper.classList.add("remove");
			forgetPassWrapper.classList.remove("remove");
		});

		document.getElementById("forgot-back").addEventListener("click", (e) => {
			passwordLoginWrapper.classList.remove("remove");
			forgetPassWrapper.classList.add("remove");
		});

		document.getElementById("password-back").addEventListener("click", (e) => {
			passwordLoginWrapper.classList.add("remove");
			magicLoginWrapper.classList.remove("remove");
			socialSignin.classList.remove("remove");
		});

		const tabAs = document.querySelectorAll(".tab-a");
		tabAs.forEach((tabA) => {
			tabA.addEventListener("click", function() {
				const tabId = this.getAttribute("data-id");
				const tabs = document.querySelectorAll(".authTab");

				tabAs.forEach((tabA) => tabA.classList.remove("active-a"));
				this.classList.add("active-a");

				tabs.forEach((tab) => {
					if (tab.dataset.id === tabId) {
						tab.classList.add("tab-active");
					} else {
						tab.classList.remove("tab-active");
					}
				});
			});
		});

		$("#loginForm").on("submit", function() {
			if (adding_favorite) {
				document.querySelector('input[name="adding_favorite"]').value = adding_favorite;
			}

			return true;
		});

		const forms = document.querySelectorAll(".authForm");

		forms.forEach((form) => {
			form.addEventListener("submit", function(event) {
				const emailInput = this.querySelector('input[name="email"]');
				if (emailInput) {
					emailInput.value = emailInput.value.trim(); // Trim leading and trailing spaces
				}
			});
		});
	});
</script>


